{
  "name": "lunchwatch-pwa",
  "version": "0.1.0",
  "description": "LunchWatch Menu Aggregator PWA",
  "author": "Joonas Tiala <joonas.tiala@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jtiala/lunchwatch-pwa"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:ts": "eslint src/**/*.ts",
    "lint:ts:fix": "eslint src/**/*.ts --fix",
    "lint:other": "prettier --list-different '**/*.{json,md,yml,yaml}'",
    "lint": "npm run lint:ts && npm run lint:other",
    "lint:fix": "npm run lint:ts:fix && format",
    "format": "prettier --write '**/*.{ts,js,json,css,scss,html,md,yml,yaml}'"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "apollo-boost": "^0.4.4",
    "date-fns": "^2.0.1",
    "graphql": "^14.5.4",
    "graphql.macro": "^1.4.2",
    "i18next": "^17.0.13",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^10.12.2",
    "react-masonry-css": "^1.0.12",
    "react-places-autocomplete": "^7.2.1",
    "react-scripts": "3.1.1",
    "typescript": "3.6.2"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.0.1",
    "@types/react-places-autocomplete": "^7.2.5",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
